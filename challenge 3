// Scores (Part 3 data)
let a = [97, 112, 101];
let b = [109, 95, 106];

let x = a.reduce((s, n) => s + n, 0) / a.length;
let y = b.reduce((s, n) => s + n, 0) / b.length;

let min = 100;

if (x === y && x >= min) {
  console.log("Both teams win the trophy 🏆");
} else if (x > y && x >= min) {
  console.log("Dolphins win the trophy 🏆");
} else if (y > x && y >= min) {
  console.log("Koalas win the trophy 🏆");
} else {
  console.log("No team wins the trophy 😢");
}
const judge = (a, b) => {
  const avg = arr => arr.reduce((s, v) => s + v) / arr.length;
  const x = avg(a), y = avg(b), m = 100;

  console.log(
    x === y && x >= m ? "Both teams win the trophy 🏆" :
    x > y && x >= m ? "Dolphins win the trophy 🏆" :
    y > x && y >= m ? "Koalas win the trophy 🏆" :
    "No team wins the trophy 😢"
  );
};

judge([97,112,101], [109,95,106]);
